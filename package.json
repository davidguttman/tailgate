{
  "name": "tailgate",
  "author": "David Guttman",
  "version": "0.0.0",
  "description": "Serve your music collection",
  "main": "index.js",
  "dependencies": {
    "async": "^0.9.0",
    "browserify": "^3.46.0",
    "coffee-script": "^1.7.1",
    "coffeeify": "^0.6.0",
    "colors": "^0.6.2",
    "connect-redis": "1.4.7",
    "directify": "^0.1.2",
    "ecstatic": "0.5.1",
    "ejs": "^1.0.0",
    "express": "~3.0.6",
    "express-zip": "^1.0.1",
    "hypnotable": "^0.6.0",
    "jade": "^1.3.1",
    "jadeify": "^2.1.1",
    "jsonist": "^0.1.1",
    "mkdirp": "^0.5.0",
    "moment": "^2.6.0",
    "redis": "^0.10.3",
    "request": "^2.36.0",
    "sassify": "^0.5.0",
    "send": "^0.1.0",
    "uglify-js": "^2.4.13",
    "underscore": "^1.6.0",
    "bean": "^1.0.6",
    "wildemitter": "^1.0.1",
    "play-audio": "^0.1.0",
    "id3js": "^1.1.3",
    "level-js": "^2.1.5",
    "levelup": "^0.18.3",
    "fastclick": "^1.0.2",
    "level": "^0.18.0",
    "emailjs": "^0.3.8",
    "cheerio": "^0.16.0"
  },
  "devDependencies": {
    "supervisor": "^0.6.0",
    "watchify": "^0.8.3"
  },
  "scripts": {
    "start": "node index.js",
    "prestart": "npm run build",
    "build": "browserify client/index.coffee | uglifyjs -mc > public/browserify.js",
    "watch-client": "watchify client/index.coffee -o public/browserify.js -dv",
    "watch-server": "supervisor -n exit -i public,client -e js,coffee index.js",
    "dev": "npm run watch-client & TAILGATE_DIR=~/storage/music npm run watch-server",
    "postinstall": "npm build"
  },
  "license": "MIT",
  "browserify": {
    "transform": [
      "coffeeify",
      "jadeify",
      "sassify"
    ]
  },
  "bin": {
    "tailgate": "./bin/tailgate"
  }
}
